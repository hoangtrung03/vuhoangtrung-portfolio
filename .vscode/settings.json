    "autofocus",
    "camelcase",
    "Chanels",
    "crumblist",
    "Cryptr",
    "customsearch",
    "deduping",
    "flowtype",
    "FOOTERYEAR",
    "formik",
    "gcse",
    "gname",
    "googlesearch",
    "gsearch",
    "hasher",
    "hookform",
    "infomation",
    "insuance",
    "largedesktop",
    "lastest",
    "leadform",
    "libphonenumber",
    "lib/utils",
    "linebreak",
    "maxage",
    "mediumdesktop",
    "mediumphone",
    "multilines",
    "oneibc",
    "oneibccom",
    "oneline",
    "paren",
    "parsetags",
    "reduxjs",
    "roachjs",
    "roboto",
    "sáàảãạăắằẳẵặâấầẩẫậéèẻẽẹêếềểễệíìỉĩịóòỏõọôốồổỗộơớờởỡợúùủũụưứừửữựýỳỷỹỵ",
    "scrolldown",
    "smalldesktop",
    "smallphone",
    "stylelint",
    "swiper",
    "Swipper",
    "tailwindcss",
    "Tiktok",
    "Touchs",
    "Trawick",
    "xlargedesktop",
    "xsmallphone"
  ],
  "[html]": {
    "editor.defaultFormatter": "vscode.html-language-features"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  }
}
